{
  "command": {
    "create": {
      "homepage": "https://github.com/lerna/lerna",
      "license": "MIT"
    },
    "publish": {
      "tempTag": true
    },
    "version": {
      "allowBranch": "main",
      "conventionalCommits": true,
      "createRelease": "github",
      "exact": true,
      "message": "chore(release): %s"
    },
  "lerna": "2.0.0-beta.31",
  "packages": [
    "packages/*"
  ],
  "changelog": {
    "repo": "davidlin88/monorepo-example",
    "labels": {
      "PR: Bug Fix": ":bug: Bug Fix",
      "PR: New Feature": ":rocket: New Features",
      "PR: Documentation": ":memo: Documentation"
    },
    "cacheDir": ".changelog"
  }
}
}
